<%- include('blocks/header', {path, auth}) %>
			<% articles.forEach(el => {%>
				<article>
					<h1><a href="/view/<%= el.id %>"><%= el.title %></a></h1>
					<%= el.user.name %> - <span class="longdate"><%= el.date %></span>
					<br>
					<%- el.content %>
				</article>
				<hr>
			<% }); %>
<% include ./blocks/footer %>