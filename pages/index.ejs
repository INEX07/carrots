<%- include('blocks/header', {path, auth}) %>
			<% articles.forEach(el => {%>
				<div class="post-info"><a href="/user/<%= el.user.username %>"><%= el.account.name %></a> - <span class="longdate"><%= el.date %></span></div>
				<article class="post">
					<header><a href="/view/<%= el.id %>"><%= el.title %></a></header>
					<main class="post"><%- el.rendered %></main>
					<footer class="post">
					  <div class="post-actions">View</div>
					  <div class="post-commentcount"><%= el.comments.length %> Comments</div>
					</footer>
				</article>
			<% }); %>
<% include ./blocks/footer %>